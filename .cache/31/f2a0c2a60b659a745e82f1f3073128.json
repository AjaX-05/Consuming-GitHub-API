"column":3}},{"generated":{"line":36,"column":3},"source":"app.js","original":{"line":57,"column":2}},{"generated":{"line":37,"column":0},"source":"app.js","original":{"line":88,"column":1}},{"generated":{"line":37,"column":1},"source":"app.js","original":{"line":54,"column":0}}],"sources":{"app.js":"const axios = require(\"axios\");\r\n\r\nconst createCard = (data) => `\r\n  <div class=\"px-4 py-5 sm:px-6 -ml-4 -mt-4 border-b border-gray-200 pb-8 flex justify-between items-center flex-wrap sm:flex-no-wrap\">\r\n    <div class=\"ml-4 mt-4\">\r\n      <div class=\"flex items-center\">\r\n        <div class=\"flex-shrink-0\">\r\n          <img class=\"h-12 w-12 rounded-full\" src=\"${data.avatar_url}\" alt=\"\">\r\n        </div>\r\n        <div class=\"ml-4\">\r\n          <h3 class=\"text-lg leading-6 font-medium text-gray-900\">\r\n            ${data.name}\r\n            <span class=\"text-sm leading-5 text-gray-500\">\r\n                @${data.login}\r\n            </span>\r\n          </h3>\r\n          <p class=\"text-sm leading-5 text-gray-500\">\r\n            ${\r\n              data.public_repos\r\n            } repositories. User since ${data.created_at.slice(0, 4)}\r\n          </p>\r\n          <p class=\"text-sm leading-5 text-gray-500\">\r\n            ${data.location || \"\"}\r\n          </p>\r\n          <p class=\"mt-1 text-sm leading-5 text-gray-500\">\r\n            ${data.bio}\r\n          </p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"ml-4 mt-4 flex-shrink-0 flex\">\r\n      <span class=\"ml-3 inline-flex rounded-md shadow-sm\">\r\n        <a href=\"${\r\n          data.html_url\r\n        }\"><button type=\"button\" class=\"mr-2 relative inline-flex items-center px-4 py-2 border border-gray-300 text-sm leading-5 font-medium rounded-md text-gray-700 bg-white hover:text-gray-500 focus:outline-none focus:shadow-outline-red focus:border-red-300 active:bg-gray-50 active:text-gray-800\">\r\n          <span>\r\n            Profile\r\n          </span>\r\n        </button>\r\n        </a>\r\n        <a href=\"${\r\n          data.blog\r\n        }\"><button type=\"button\" class=\"relative inline-flex items-center px-4 py-2 border border-gray-300 text-sm leading-5 font-medium rounded-md text-gray-700 bg-white hover:text-gray-500 focus:outline-none focus:shadow-outline-red focus:border-red-300 active:bg-gray-50 active:text-gray-800\">\r\n          <span>\r\n            Website\r\n          </span>\r\n        </button>\r\n        </a>\r\n      </span>\r\n    </div>\r\n  </div>\r\n`;\r\n\r\ndocument.addEventListener(\"DOMContentLoaded\", () => {\r\n  const usernames = [];\r\n  const form = document.querySelector(\"form\");\r\n  form.addEventListener(\"submit\", async (event) => {\r\n    event.preventDefault();\r\n\r\n    const username = document.querySelector(\"input\").value;\r\n    if (usernames.includes(username)) {\r\n      alert(\"You already Searched for This\");\r\n      return;\r\n    }\r\n    usernames.push(username);\r\n\r\n    document.querySelector(\"input\").value = \"\";\r\n\r\n    let response = \"\";\r\n    try {\r\n      response = await axios.get(`https://api.github.com/users/${username}`);\r\n    } catch (error) {\r\n      if (404 === error.response.status) {\r\n        alert(\"Username not Found!\");\r\n      } else {\r\n        alert(\"Error\");\r\n        console.log(error.response);\r\n      }\r\n    }\r\n\r\n    if (response) {\r\n      const card = createCard(response.data);\r\n      document\r\n        .querySelector(\"#container\")\r\n        .insertAdjacentHTML(\"afterbegin\", card);\r\n    }\r\n  });\r\n});\r\n"},"lineCount":null}},"error":null,"hash":"eea20f45f3aec5f04f0c3573ab6498d7","cacheData":{"env":{}}}